/* 底部淡入 */
.coup-anim {
	-webkit-animation: fadeIn 0.4s .1s ease both;
	        animation: fadeIn 0.4s .1s ease both;
}
@-webkit-keyframes fadeIn {
	0% {
		opacity: 0;
		-webkit-transform: translateY(400rpx);
		        transform: translateY(400rpx)
	}

	100% {
		opacity: 1;
		-webkit-transform: translateY(0);
		        transform: translateY(0)
	}
}
@keyframes fadeIn {
	0% {
		opacity: 0;
		-webkit-transform: translateY(400rpx);
		        transform: translateY(400rpx)
	}

	100% {
		opacity: 1;
		-webkit-transform: translateY(0);
		        transform: translateY(0)
	}
}
.skeleton-index{
position: fixed;
top: 0;
left: 0;
right: 0;
bottom: 0;
background-color: #FFFFFF;
z-index: 99;
overflow: hidden;
}
